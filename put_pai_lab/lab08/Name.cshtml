@{
    var name = Request["name"];
    if (string.IsNullOrEmpty(name))
    {
        name = "Robert Paulson";
    }
}
<!DOCTYPE html>
<html>
<head>
    <title>Name Page</title>
</head>
<body>
    <ul>
        @for (int i = 0; i < 10; i++)
        {
            <li>His name is @name</li>
        }
    </ul>
</body>
</html>
